{
  "name": "jw-react-workflow",
  "version": "0.0.2",
  "private": true,
  "description": "Personal React Redux Boilerplate",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon --watch webpack.config.babel.js --exec \"webpack-dev-server\"",
    "build": "rimraf lib dist && babel app -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "serve": "yarn build; serve ./dist -s -o",
    "lint": "eslint app webpack.config.babel.js --ext .js,.jsx",
    "fix": "eslint app webpack.config.babel.js --ext .js,.jsx --fix",
    "lint:css": "stylelint ./app/**/*.scss",
    "format": "yarn format:css && yarn format:js",
    "format:css": "prettier-stylelint --write -q \"app/sharedStyles/**/*.scss\"",
    "format:js": "prettier --write app/**/*.js webpack.config.babel.js",
    "stylelint-find-unused-rules": "stylelint-find-rules --config .stylelintrc",
    "eslint-check": "eslint --print-config ./.eslintrc | eslint-config-prettier-check",
    "test": "yarn lint && jest --coverage",
    "precommit": "lint-staged"
  },
  "engines": {
    "yarn": ">= 1.0.2"
  },
  "browserslist": [
    "> 1%"
  ],
  "keywords": [
    "react",
    "redux",
    "react-redux",
    "boilerplate",
    "react-router"
  ],
  "author": "justin woodward <justin@justinwoodward.com>",
  "dependencies": {
    "axios": "^0.17.1",
    "immutable": "^3.8.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "axios-mock-adapter": "^1.12.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-plugin-transform-remove-console": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-3": "^6.24.1",
    "browser-sync": "^2.23.6",
    "browser-sync-webpack-plugin": "^2.0.1",
    "connected-react-router": "^4.3.0",
    "copy-webpack-plugin": "^4.3.1",
    "core-js": "^2.5.3",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-problems": "^0.0.2",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^1.1.6",
    "happypack": "^4.0.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.0.0",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "jest": "^22.1.4",
    "lint-staged": "^6.1.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.11",
    "postcss-loader": "^2.0.10",
    "postcss-pxtorem": "^4.0.1",
    "prettier": "^1.10.2",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.0",
    "prettier-eslint-webpack-plugin": "^0.14.73",
    "prettier-stylelint": "^0.4.2",
    "prop-types": "^15.5.10",
    "redux-devtools": "^3.4.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "serve": "^6.4.9",
    "style-loader": "^0.20.1",
    "stylelint": "^8.4.0",
    "stylelint-config-idiomatic-order": "^5.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-find-rules": "^1.1.0",
    "stylelint-scss": "^2.2.0",
    "stylelint-webpack-plugin": "^0.10.1",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-chunk-hash": "^0.5.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "modulePaths": [
      "node_modules",
      "app"
    ]
  },
  "lint-staged": {
    "app/**/*.js": [
      "yarn fix",
      "yarn format:js",
      "git add"
    ],
    "app/sharedStyles/**/*.scss": [
      "yarn format:css",
      "yarn lint:css",
      "git add"
    ]
  }
}
